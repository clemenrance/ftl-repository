pipeline{

    agent any

    stages{

        stage('git checkout'){
            
            steps{
                sh 'mvn test'
            }
        }
        stage('integration test'){

            steps{
                sh 'mvn verify -DskipUnitTests'
            }
        }
        stage('MAVEN BUILD'){

            steps{
                sh 'mvn clean install'
            }
        }
            }
        }
    }
}